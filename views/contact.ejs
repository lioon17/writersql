  <!-- Contact Us Header Section -->
  <!-- Contact Header Section -->
   <div class="contact-container">
<div class="custom-contact-header">
    <h2>Contact Us</h2>
    <nav>
        <ul>
            <li><a href="#">Home</a> &gt;</li>
            <li><a href="#">Contact Us</a></li>
        </ul>
    </nav>
</div>

<!-- Contact Info Section -->
<div class="custom-contact-info">
    <div class="custom-info-box">
        <i class="fas fa-envelope"></i>
        <p>Support</p>
        <span>support@yoursite.com</span>
    </div>
    <div class="custom-info-box">
        <i class="fas fa-phone"></i>
        <p>Phone</p>
        <span>+1 (818) 333 44 55</span>
    </div>
    <div class="custom-info-box">
        <i class="fas fa-map-marker-alt"></i>
        <p>Address</p>
        <span>121 King Street, Los Angeles</span>
    </div>
</div>

<!-- Contact Form Section -->
<div class="custom-contact-form">
    <div class="custom-form-wrapper">
        <h2>Get in touch with us</h2>
        <form>
            <div class="custom-field-container">
                <label for="full-name">Full Name:</label>
                <input type="text" id="full-name" name="full-name">
            </div>
            <div class="custom-field-container">
                <label for="phone">Your Phone:</label>
                <input type="text" id="phone" name="phone">
            </div>
            <div class="custom-field-container">
                <label for="date">Date:</label>
                <input type="text" id="date" name="date">
            </div>
            <div class="custom-field-container">
                <label for="sickness">:</label>
                <input type="text" id="sickness" name="sickness">
            </div>
            <div class="custom-field-container">
                <label for="sickness-explanation">:</label>
                <textarea id="sickness-explanation" name="sickness-explanation"></textarea>
            </div>
            <button type="submit">Send request</button>
        </form>
    </div>
    <div class="custom-map-container">
        <iframe 
            src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d3021.826849784819!2d-74.003267184595!3d40.71278427933033!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x89c25a19e1515c4f%3A0x5b2ddbd7056b309f!2sNew%20York%2C%20NY%2010002%2C%20USA!5e0!3m2!1sen!2sin!4v1583229571702!5m2!1sen!2sin" 
            width="600" height="450" frameborder="0" style="border:0;" allowfullscreen="" aria-hidden="false" tabindex="0"></iframe>
    </div>
</div>

<!-- FAQ Section -->
<div class="custom-faq-section">
    <h2>Frequently Asked Questions</h2>
    <div class="custom-faq">
        <h3>What services do you offer?</h3>
        <p>We offer a variety of services including consultation, customer support, and more.</p>
    </div>
    <div class="custom-faq">
        <h3>How can I contact support?</h3>
        <p>You can contact support via email at support@yoursite.com or by phone at +1 (818) 333 44 55.</p>
    </div>
    <div class="custom-faq">
        <h3>Where are you located?</h3>
        <p>We are located at 121 King Street, Los Angeles.</p>
    </div>
</div>

</div>
